
      
      <% if (inputName) { %> 
      <form id="<%= formid %>">
   
              <div class="row-g-3">


 
    <%  var arr=inputName,
        i = 0,
        len = arr.length; %>

       <% while(i < len){ %>

       <%- include(`${partials}/form/inputcol`, {inputName:{name: arr[i].name, type: arr[i].type} }); -%>
       <% 
        // some js in the loop
        console.log('arr[' + i + '] = ' + arr[i]);
        i += 1; 
         %>
       <% } %>

    
       <hr class="my-4">
 <% if (button) { %>
    <button
    class="btn btn-primary"
  
    type=" <%= button.type %>"
    >
    <%= button.name %>
</button>
 
<% } %>

         


</div>
                

        
       </form>
   <% } %>
 
 
